{
  "extends": ["wesbos"],
  "rules": {
    "no-console": 2,
    "import/no-cycle": "off",
    "no-use-before-define": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 4
      }
    ]
  }
}
